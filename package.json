{
  "name": "newerahockey",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "vite build",
    "preview": "vite preview",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "lint": "eslint src --ext js,jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css}\"",
    "prepare": "node -e \"try { require('husky') } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\"",
    "git:r": "git checkout master && git fetch && git pull origin master"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.978.0",
    "@aws-sdk/client-ses": "^3.978.0",
    "@aws-sdk/util-dynamodb": "^3.0.0",
    "@netlify/blobs": "^10.1.0",
    "axios": "^1.6.7",
    "date-fns": "^3.6.0",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.8",
    "google-auth-library": "^10.4.1",
    "googleapis": "^163.0.0",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.4",
    "stripe": "^19.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css}": [
      "prettier --write"
    ]
  }
}
